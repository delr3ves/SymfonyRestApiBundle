{
  "name": "delr3ves/restapi-bundle",
  "description": "Rest API bundle for Symfony2",
  "keywords": ["restapi", "symfony"],
  "homepage": "https://github.com/wopp-me/keyvalue-bundle",
  "type": "symfony-bundle",
  "license": "MIT",
  "minimum-stability": "stable",
  "authors": [
    {
      "name": "Sergio Arroyo Cuevas",
      "email": "sergio.arroyo.cuevas@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.3",
    "symfony/symfony": ">=2.3",
    "doctrine/annotations": "1.2.6"
  },
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "mockery/mockery": "0.7.2",
    "satooshi/php-coveralls": "1.0.1",
    "phpunit/phpcov": "2.*"
  },
  "autoload": {
    "psr-0": { "Delr3ves\\RestApiBundle": "" }
  },
  "target-dir": "Delr3ves/RestApiBundle",
  "config": {
    "bin-dir": "bin"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}